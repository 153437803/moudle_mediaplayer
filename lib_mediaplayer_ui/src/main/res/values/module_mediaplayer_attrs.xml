<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MPLoadingView">
        <attr name="lv_radius" format="dimension|reference" />
        <attr name="lv_rate" format="float|reference" />
        <attr name="lv_count" format="integer" />
        <attr name="lv_color_background" format="color|reference" />
        <attr name="lv_color_round" format="color|reference" />
        <attr name="lv_delay_millis" format="integer" />
    </declare-styleable>
    <declare-styleable name="AnimationProgress">
        <attr name="ap_background_color" format="color|reference" />
        <attr name="ap_progress_color" format="color|reference" />
        <attr name="ap_line_color" format="color|reference" />
        <attr name="ap_line_space" format="dimension|reference" />
        <attr name="ap_line_width" format="dimension|reference" />
        <attr name="ap_progress" format="integer" />
        <attr name="ap_animator_time" format="integer" />
        <attr name="ap_animator_progress_time" format="integer" />
    </declare-styleable>
</resources>